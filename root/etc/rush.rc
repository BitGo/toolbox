debug 1

rule default
  acct on
  limits t10r20
  umask 002
  env - USER LOGNAME HOME PATH LOGIN_FINGERPRINT LOGIN_EMAIL LOGIN_NAME
  fall-through

rule login-fingerprint
  command ^login-fingerprint$
  set[0] /usr/local/bin/login-fingerprint

rule login-email
  command ^login-email$
  set[0] /usr/local/bin/login-email

rule login-name
  command ^login-name$
  set[0] /usr/local/bin/login-name
